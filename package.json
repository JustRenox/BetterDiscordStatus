{
  "name": "betterdiscordstatus",
  "productName": "BetterDiscordStatus",
  "version": "1.0.0",
  "description": "A simple electron app to give users the ability to have a nicer looking discord status",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JustRenox/BetterDiscordStatus.git"
  },
  "author": "Renox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JustRenox/BetterDiscordStatus/issues"
  },
  "homepage": "https://github.com/JustRenox/BetterDiscordStatus#readme",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "betterdiscordstatus"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "bootstrap-icons": "^1.3.0",
    "discord-rpc": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "11.2.3"
  }
}
